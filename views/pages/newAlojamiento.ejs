<form id="formLogin" method="POST" action="/newAlojamiento"
      class="d-flex flex-column text-center justify-content-center align-items-center pt-5 pb-5">

    <div class="form-group w-25">
        <label for="nombre"><b>Nombre</b></label>
        <input type="text" class="form-control" id="nombre" name="nombre">
    </div>

    <div class="form-group w-25">
        <label for="capacidad"><b>Capacidad</b></label>
        <input type="text" class="form-control" id="capacidad" name="capacidad">
    </div>

    <div class="form-group w-25">
        <label for="garage"><b>Garage (true or false)</b></label>
        <br>
        <input type="radio" name="garage" value="true" class="mt-2"> SI
        <input type="radio" name="garage" value="false" class="ml-5"> NO
    </div>

    <div class="form-group w-25">
        <label for="imagen"><b>Imagen (128px)</b></label>
        <input type="text" class="form-control" id="imagen" name="imagen">
    </div>

    <div class="form-group w-25">
        <label for="direccion"><b>Direcci√≥n</b></label>
        <input type="text" class="form-control" id="direccion" name="direccion">
    </div>

    <button type="submit" class="btn btn-primary btnAlojamiento">Crear</button>

</form>
